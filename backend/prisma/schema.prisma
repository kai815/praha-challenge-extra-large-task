datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  previewFeatures = ["nativeTypes"]
  provider        = "prisma-client-js"
}

model SomeData {
  id       String  @id
  required Boolean
  number   Int     @db.SmallInt
}

model User {
  id     String @id
  name   String
  email  String @unique
  status Status @default(Inmembership)
}

enum Status {
  Inmembership
  Inactive
  Withdrawn
}
